Interface interface: {
    Height : 400,
    Width  :500,
    Title : "App",
    LetterSpacing : 1,
    MarginLeft : 54
};
Label label: {
    Height : 40,
    Width  :20,
    Opacity : 100,
    Text : "label here",   
    FontStyle : "normal",
    FontWeight : "bolder",
    LetterSpacing : 1,
    MarginLeft : 54
};

Label label2: {
    Height : 40,
    Width  :20,
    Opacity : 50,
    Text : "label here too but with less opacity"
};

Button button: {
    Height : 40,
    Width  :20,
    Opacity : 100,
    Text : "Click Me"
};

InputField input: {
    Height : 40,
    Width  :20,
    Opacity : 100,
    PlaceHolder : "Enter your Email"
};



Begin
Start interface "CENTER" ;
Put label 10 0;
Put label2 50 50;
Put button 10 200;
Put input 100 100;
Show interface;
End